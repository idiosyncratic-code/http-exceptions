{
  "name": "idiosyncratic/http-exceptions",
  "type": "library",
  "license": "ISC",
  "authors": [
    {
      "name": "Jason Silkey",
      "email": "jason@jasonsilkey.com"
    }
  ],
  "keywords": [
  ],
  "require": {
    "php": ">=7.3",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "idiosyncratic/coding-standard": "^1.0",
    "jakub-onderka/php-console-highlighter": "^0.4.0",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "phpstan/phpstan": "^0.11",
    "phpunit/phpunit": "^8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "discard-changes": true,
    "bin-dir" : "bin"
  },
  "autoload": {
    "psr-4": {
      "Idiosyncratic\\Http\\Exception\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Idiosyncratic\\Http\\Exception\\": "tests"
    }
  }
}
